{  "version": "1.0.0",  "description": "Boilerplate",  "name": "Boilerplate",  "manifest_version": 3,  "permissions": [    "storage",    "declarativeNetRequest"  ],  "declarative_net_request": {    "rule_resources": [      {        "id": "ruleset_1",        "enabled": true,        "path": "rules.json"      }    ]  },  "icons": {    "16":"icon.png",    "48": "icon.png",    "128": "icon.png"  },  "background": {    "service_worker": "background.js"  },  "content_scripts": [    {      "matches": ["<all_urls>"],      "js": ["contentScript.js"]    }  ]}